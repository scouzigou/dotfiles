# tmux zsh plugin
#export ZSH_TMUX_AUTOSTART="true" # useless because using theme powerlevel10k that require to manually start tmux session (see first lines of .zshrc)
export ZSH_TMUX_AUTOQUIT="false"

# fnm (used by node)
# see https://nodejs.org/en/download/package-manager
export PATH="$HOME/.local/share/fnm:$PATH"
eval "`fnm env`"

# cargo (used when building & installing directly binary from RUST)
export PATH="$HOME/.cargo/bin:$HOME/.local/bin:$PATH"

